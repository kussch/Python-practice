import requests
from bs4 import BeautifulSoup


url = 'https://www.weblancer.net/jobs/'
page = requests.get(url).text
soup = BeautifulSoup(page, 'html.parser')
divs = soup.findAll('div', {'class': 'dropdown-menu block-content text_field'})


def find_categories():
    for div in divs:
        li_category = div.find('div', {'class': 'list-unstyled list-wide category_tree'}) 
        li_text = li_category.find('href').text
        print(li_text)
       
       
 print(find_categories())

